{
  "$schema": "../../../schemas/toolMetadata.json",
  "id": "pastebin_get_raw_content",
  "description": "Navigate to the raw version of the current paste",
  "groupId": "pastebin",
  "filters": [
    {
      "type": "domain",
      "domains": ["pastebin.com", "*.pastebin.com"]
    },
    {
      "type": "path",
      "patterns": ["/.*"]
    }
  ]
}
