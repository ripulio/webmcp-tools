{
  "$schema": "../../../schemas/toolMetadata.json",
  "id": "google_docs_get_document_title",
  "description": "Retrieve the current document's title",
  "groupId": "google_docs",
  "filters": [
    {
      "type": "domain",
      "domains": ["docs.google.com"]
    },
    {
      "type": "path",
      "paths": ["/document/d/[^/]+/edit.*"]
    }
  ]
}
