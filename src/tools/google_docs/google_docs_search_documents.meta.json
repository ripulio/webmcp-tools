{
  "$schema": "../../../schemas/toolMetadata.json",
  "id": "google_docs_search_documents",
  "description": "Search for documents by name in Google Docs",
  "groupId": "google_docs",
  "filters": [
    {
      "type": "domain",
      "domains": ["docs.google.com"]
    },
    {
      "type": "path",
      "patterns": ["/document/u/.*", "/document/?$"]
    }
  ]
}
